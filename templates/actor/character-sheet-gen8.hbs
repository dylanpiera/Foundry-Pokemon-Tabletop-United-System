                          <option value="blank">{{localize "PTU.SelectNone"}}</option>
                          {{#each skills as | skill key |}}
                          <option value="{{key}}">{{skill.label}}</option>
                          {{/each}}
                        </select>
                        {{/select}}
                      </div>
                    </div>
                  </div>
                  <div class="swsh-header">
                    <div class="d-flex w-100 mt-1 mb-1">
                      <div class="col-sm-6">
                        <label for="system.background.novice">{{localize "PTU.SkillNovice"}} {{localize "PTU.Skill"}}</label>
                      </div>
                      <div class="col-sm-6">
                        <label for="system.background.adept">{{localize "PTU.SkillAdept"}} {{localize "PTU.Skill"}}</label>
                      </div>
                    </div>
                  </div>
                  <div class="swsh-body">
                    <div class="d-flex w-100 mt-1 mb-1 fs-9">
                      <div class="col-sm-6">
                        {{#select data.background.novice}}
                        <select name="system.background.novice">
                          <option value="blank">{{localize "PTU.SelectNone"}}</option>
                          {{#each skills as | skill key |}}
                          <option value="{{key}}">{{skill.label}}</option>
                          {{/each}}
                        </select>
                        {{/select}}
                      </div>
                      <div class="col-sm-6">
                        {{#select data.background.adept}}
                        <select name="system.background.adept">
                          <option value="blank">{{localize "PTU.SelectNone"}}</option>
                          {{#each skills as | skill key |}}
                          <option value="{{key}}">{{skill.label}}</option>
                          {{/each}}
                        </select>
                        {{/select}}
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="col h-100">
                <!-- Biography -->
                <div class="swsh-box mb-2"
                    style="text-align: center; word-break: keep-all;">
                  <div class="swsh-header">
                    <div class="d-flex w-100 mt-1 mb-1">
                      <div class="col-sm-3">
                        <label for="system.age">{{localize "PTU.Age"}}:</label>
                      </div>
                      <div class="col-sm-3">
                        <label for="system.sex">{{localize "PTU.Sex"}}:</label>
                      </div>
                      <div class="col-sm-3">
                        <label for="system.height">{{localize "PTU.Height"}}:</label>
                      </div>
                      <div class="col-sm-3">
                        <label for="system.weight">{{localize "PTU.Weight"}}:</label>
                      </div>
                    </div>
                  </div>
                  <div class="swsh-body">
                    <div class="d-flex w-100 mt-1 mb-1">
                      <div class="col-sm-3">
                        <input
                          name="system.age"
                          type="text"
                          value="{{data.age}}"
                          data-dtype="Number"
                        />
                      </div>
                      <div class="col-sm-3">
                        <input
                          type="text"
                          name="system.sex"
                          value="{{data.sex}}"
                        />
                      </div>
                      <div class="col-sm-3">
                        <input
                          type="text"
                          name="system.height"
                          value="{{data.height}}"
                        />
                      </div>
                      <div class="col-sm-3">
                        <input
                          type="text"
                          name="system.weight"
                          value="{{data.weight}}"
                        />
                      </div>
                    </div>
                  </div>
                </div>
                <div class="swsh-box h-25 w-100 mb-2 red">
                  <div class="swsh-header">
                    <h3>{{localize "PTU.PhysicalDescription"}}</h3>
                  </div>
                  <div class="swsh-body h-90">
                    {{editor data.biography.physicalDescription target="system.biography.physicalDescription" button=true owner=owner editable=editable}}
                  </div>
                </div>
                <div class="swsh-box h-35 w-100 mb-2 yellow">
                  <div class="swsh-header">
                    <h3>{{localize "PTU.Background"}}</h3>
                  </div>
                  <div class="swsh-body h-90">
                    {{editor data.biography.background target="system.biography.background" button=true owner=owner editable=editable}}
                  </div>
                </div>
                <div class="swsh-box h-35 w-100 mb-2 purple">
                  <div class="swsh-header">
                    <h3>{{localize "PTU.Personality"}}</h3>
                  </div>
                  <div class="swsh-body h-90">
                    {{editor data.biography.personality target="system.biography.personality" button=true owner=owner editable=editable}}
                  </div>
                </div>
                <div class="swsh-box h-35 w-100 mb-2 green">
                  <div class="swsh-header">
                    <h3>{{localize "PTU.Goals"}}</h3>
                  </div>
                  <div class="swsh-body h-90">
                    {{editor data.biography.goals target="system.biography.goals" button=true owner=owner editable=editable}}
                  </div>
                </div>
                <div class="swsh-box h-35 w-100 mb-2 green">
                  <div class="swsh-header">
                    <h3>{{localize "PTU.Dreams"}}</h3>
                  </div>
                  <div class="swsh-body h-90">
                    {{editor data.biography.dreams target="system.biography.dreams" button=true owner=owner editable=editable}}
                  </div>
                </div>
                <div class="swsh-box h-35 w-100 mb-2 green">
                  <div class="swsh-header">
                    <h3>{{localize "PTU.Obsessions"}}</h3>
                  </div>
                  <div class="swsh-body h-90">
                    {{editor data.biography.obsessions target="system.biography.obsessions" button=true owner=owner editable=editable}}
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- Dex -->
          <div class="tab details dex" data-group="primary" data-tab="dex">
            <div class="row">
              <div class="col-6">
                <div class="swsh-box mt-1">
                    <div class="swsh-header d-flex justify-content-around sort-dex" >
                      <div class="p-1">
                        <button class="btn rollable sort-dex" data-sort="name">Sort Name</button>
                      </div>
                      <div class="p-1">
                        <button class="btn rollable sort-dex" data-sort="id">Sort Id</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="row">
              <!-- Seen -->
              <div class="col">
                <div class="swsh-box mt-2 w-100">
                  <div class="swsh-body w-100" style="display: block;">
                    {{> "systems/ptu/templates/partials/item-display-partial.hbs" title=(localize "ITEM.DexEntrySeen") type="dexentry" data=actor.dex.seen}}
                  </div>
                </div>
              </div>
              <!-- Owned -->
              <div class="col">
                <div class="swsh-box mt-2 w-100">
                  <div class="swsh-body w-100" style="display: block;">
                    {{> "systems/ptu/templates/partials/item-display-partial.hbs" title=(localize "ITEM.DexEntryOwned") type="dexentry" data=actor.dex.owned}}
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- Pokenav -->
          <div class="tab details pokenav" data-group="primary" data-tab="pokenav">
            <div class="row">
              <div class="col">
                <div class="swsh-box mt-1 w-100">
                    <div class="swsh-header d-flex justify-content-around sort-nav" >
                      <div class="p-1">
                        <button class="btn rollable sort-nav" data-sort="name">Sort Name</button>
                      </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="row">
              <!-- Contacts -->
              <div class="col">
                <div class="swsh-box mt-2 w-100">
                  <div class="swsh-body w-100" style="display: block;">
                    {{> "systems/ptu/templates/partials/item-display-partial.hbs" title=(localize "ITEM.Contacts") type="contact" data=actor.contacts}}
                  </div>
                </div>
              </div>
            </div>
          </div>

          <!-- Effects Tab -->
          <div class="tab details effects" data-group="primary" data-tab="effects">
            <div class="h-100 swsh-box">
              <div class="swsh-header pl-2 effect-header">
                <h3 style="flex: 1 1 50%">{{localize "PTU.ActiveEffects"}}</h3>
                <div class="item-controls pr-1 flexcol" style="flex: 0 0 7%;">
                  <a class="item-control item-create" data-type="ActiveEffect" title="Add Effect"><i
                      class="fas fa-plus-circle" style="margin-right: 3px;"></i><span
                      class="readable">{{localize "ITEM.AddButton"}}</span></a>
              </div>
              </div>
              <div class="swsh-body pt-3 p-2">
                {{> "systems/ptu/templates/partials/active-effects.hbs"}}
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="sheet-footer">
    <div class="tabs">
      <ul class="sheet-tabs tabs buttons" data-group="primary">
        <li><button class="item" data-tab="stats">{{localize "PTU.TabsSkills"}}</button></li>
        <li><button class="item" data-tab="inventory">{{localize "PTU.TabsInventory"}}</button></li>
        <li><button class="item" data-tab="features">{{localize "PTU.TabsFeatsAndEdges"}}</button></li>
        <li><button class="item" data-tab="combat">{{localize "PTU.TabsCombat"}}</button></li>
        <li><button class="item" data-tab="moves">{{localize "PTU.TabsMoves"}}</button></li>
        <li><button class="item" data-tab="extra">{{localize "PTU.TabsExtra"}}</button></li>
        <li><button class="item" data-tab="dex">{{localize "PTU.TabsDex"}}</button></li>
        {{#if (is true (getGameSetting "showPokNavTab"))}}
          <li><button class="item" data-tab="pokenav">{{localize "PTU.TabsNav"}}</button></li>
        {{/if}}
        <li><button class="item" data-tab="effects">{{localize "PTU.TabsEffects"}}</button></li>
      </ul>
    </div>
  </div>
</form>
